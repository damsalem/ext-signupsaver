{
	"manifest_version": 3,
	"name": "SignUpSaver",
	"description": "Save a SignUp from SignUpGenius",
	"version": "1.0.0",
	"action": {
		"default_popup": "popup/popup.html"
	},
	"permissions": ["activeTab", "bookmarks"],
	"icons": {
		"16": "images/logo-16.png",
		"32": "images/logo-32.png",
		"48": "images/logo-48.png",
		"128": "images/logo-128.png"
	}
}
